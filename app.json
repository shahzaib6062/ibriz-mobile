{
  "expo": {
    "name": "ibriz-mobile",
    "slug": "ibriz-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "height": "100px",
      "width": "100px",
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0432FF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Splash.png",
        "backgroundColor": "#0432FF",
        "height": "100px",
        "width": "100px"
      }
    },
    "web": {
      "favicon": "./assets/IBRIZ_logo.svg"
    },
    "plugins": ["expo-router"]
  }
}
